<% if(renderProductos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: green"></tr>
            <th>Title</th>
            <th>price</th>
            <th>thumbnail</th>
            <th>stock</th>
            <th>id</th>
            <th></th>
            <th></th>

                <% renderProductos.forEach(function(producto){%>
            <tr>

            <td><%=producto.title%></td>
            <td><%=producto.price%></td>
            <td><img src="<%=producto.thumbnail%>" alt="" style: width="100"></td>
            <td><%=producto.stock%></td>
            <td><%=producto.id%></td>
            <td><form action= "/api/borrar/<%=producto.title%>" method="GET">
                <button class="btn btn-danger mt-3 mb-5">Eliminar</button>
            </td>
            <td></form>
                <form action= "api/carrito/agregarproducto/<%=producto.title%>" method="POST">
                    <button class="btn btn-secondary mt-3 mb-5">Agregar al carrito</button>
                </form>
            </td>
        </tr>
        <% });%>
    </table>
    </div>
        <% } else { %>
            <h2>no hay datos</h2>
        <% } %>